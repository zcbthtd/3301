#!/bin/bash
#SBATCH -t 1:00:00
#SBATCH -n 24
#SBATCH -p short
module load eb
module load Miniconda2
source activate qiime1
export TMPDIR=~/qiime_tmp
echo "demultiplexing"
split_libraries_fastq.py \
-m map_incomplete.tsv \
-i seqprep_assembled.fastq.gz \
-b seqprep_assembled.fastq_barcodes.fastq \
-o ./slout \
-q 19 \
--rev_comp_barcode \
--rev_comp_mapping_barcodes
source deactivate
